    "mobx-react": "^6.1.4",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "resium": "^1.10.0"
  },
  "browserslist": {
    "production": [
      "last 2 version",
      "> 1%",
      "ie >= 11"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "git add"
    ],
    "src/**/*.{js,jsx,ts,tsx,json,css,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-decorators": "^7.7.4",
    "@craco/craco": "^5.6.2",
    "@types/cesium": "^1.59.3",
    "@types/jest": "^24.0.23",
    "@types/memoize-one": "^5.1.2",
    "@types/node": "^12.12.14",
    "@types/react": "^16.9.15",
    "@types/react-dom": "^16.9.4",
    "babel-plugin-import": "^1.13.0",
    "craco-cesium": "^1.2.0",
    "craco-plugin-react-hot-reload": "^0.1.0",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-prettier": "^3.1.1",
    "husky": "^3.1.0",
    "lint-staged": "^9.5.0",
    "prettier": "^1.19.1",
    "react-hot-loader": "^4.12.18",
    "react-scripts": "^3.3.0",
    "typescript": "^3.7.3"
  }
