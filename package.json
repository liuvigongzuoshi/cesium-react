{
  "name": "esium-react",
  "version": "1.0.0",
  "private": true,
  "homepage": ".",
  "scripts": {
    "start": "craco start",
    "dev": "craco start",
    "build": "craco build",
    "test": "craco test",
    "lint": "eslint --ext tsx,ts,js,jsx src",
    "lint-fix": "eslint --fix --ext tsx,ts,js,jsx src",
    "prettier": "prettier -c --write '**/*'",
    "precommit": "lint-staged"
  },
  "dependencies": {
    "@hot-loader/react-dom": "^16.11.0",
    "antd": "^3.26.1",
    "cesium": "^1.64.0",
    "memoize-one": "^5.1.1",
